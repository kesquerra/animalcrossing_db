<table class="table table-striped">
    <thead>
        <tr>
            {{#each column_name}}
                <th>{{this}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each record}}
            <tr>
                {{#each ../column_name}}
                    <td>{{lookup .. this}}</td>
                {{/each}}
                <td><button>Update</button></td>
                <td><button>Delete</button></td>
            </tr>
        {{/each}}
    </tbody>
</table>